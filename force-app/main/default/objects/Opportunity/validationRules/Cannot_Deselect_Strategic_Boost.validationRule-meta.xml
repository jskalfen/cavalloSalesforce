<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Deselect_Strategic_Boost</fullName>
    <active>true</active>
    <errorConditionFormula>PRIORVALUE(Strategic_Boost__c) = true
&amp;&amp; Strategic_Boost__c = false
&amp;&amp; $User.Full_Name__c &lt;&gt; &apos;Rosewater Admin&apos;</errorConditionFormula>
    <errorDisplayField>Strategic_Boost__c</errorDisplayField>
    <errorMessage>Cannot deselect Strategic Boost. Please reach out to your Salesforce Admin to resolve.</errorMessage>
</ValidationRule>
