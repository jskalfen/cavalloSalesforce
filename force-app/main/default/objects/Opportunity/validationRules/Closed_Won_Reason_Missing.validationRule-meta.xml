<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closed_Won_Reason_Missing</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISBLANK( TEXT(Closed_Won_Reason__c)), ISPICKVAL(StageName, &quot;Closed Won&quot;))</errorConditionFormula>
    <errorMessage>Closed Won Reason must be filled out</errorMessage>
</ValidationRule>
