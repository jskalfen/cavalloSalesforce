<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Account_Name_from_being_Blanked</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK(PRIORVALUE(AccountId)))
&amp;&amp;
ISBLANK(AccountId)</errorConditionFormula>
    <errorMessage>Account Id cannot be blank</errorMessage>
</ValidationRule>
